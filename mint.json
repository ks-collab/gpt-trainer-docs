{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Starter Kit",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#9563FF",
    "light": "#AE87FF",
    "dark": "#0D001D",
    "background": {
      "dark": "#090014"
    },
    "anchors": {
